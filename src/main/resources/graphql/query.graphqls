type StatusResponse{
    resource: String
    finish: String
    kind: String
}
type Status{
    completed: Boolean
    status: [StatusResponse]
}
input SubNet{
    name: String
    cidr: String
}
input StackInfraInput{
    environment: String
    accessKey: String
    secretKey: String
    route53Domain: String
    instanceType: String
    desiredSize: String
    maxSize: String
    minSize: String
    vpc: String
    subnet: [SubNet]
}

type Query{
    status: Status
}
type Mutation{
    verify(accessKey: String, secretKey: String): String
    stackInfra(stackInfraInput: StackInfraInput!): String
}
